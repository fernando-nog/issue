SELECT
	customer_details_.`id`,
	customer_details_.`customerId`,
	customer_details_.`categoryId`,
	customer_details_.`label`,
	customer_details_.`detail`,
	customer_details_.`createdAt`,
	customer_details_.`updatedAt`,
	customer_details_category_.`name` AS category_name,
	customer_details_category_.`active` AS category_active,
	customer_details_category_.`createdAt` AS category_createdAt,
	customer_details_category_.`priority` AS category_priority,
	customer_details_category_active_.`name` AS category_active_name,
	customer_details_category_active_.`active` AS category_active_active,
	customer_details_category_active_.`createdAt` AS category_active_createdAt,
	customer_details_category_active_.`priority` AS category_active_priority,
	customer_details_customer_.`name` AS customer_name,
	customer_details_customer_.`createdAt` AS customer_createdAt,
	customer_details_customer_.`showCustomer` AS customer_showCustomer
FROM
	`CustomerDetails` customer_details_
INNER JOIN `Category` customer_details_category_ ON
	customer_details_.`categoryId`= customer_details_category_.`id`
INNER JOIN `Customer` customer_details_customer_ ON
	customer_details_.`customerId`= customer_details_customer_.`id`
WHERE
	(customer_details_.`customerId` = ?
		AND customer_details_category_.`active` = ?)


